\nblockquote{\n    font-size: 20px;\n    font-style: italic;\n    color:#7e7d7d;\n}\nblockquote a {\n    margin-left: 15px;\n    font-weight: 400;\n\n}\n#next{\n    position: relative;\n    float:right;\n    bottom: 50px;\n    right:10px;\n    font-weight: 800;\n\n}\n#previous{\n    font-weight: 800;\n    margin: 10px;\n}\n\n\n/* general table setting */\n\ntable{\n    border-collapse:collapse\n}\ntd,th{\n    padding:5px;\n    border:1px solid #000;\n}\ntable caption{\n    color:#433D86;\n    background-color:#C0BBFF;\n    border: 1px solid #433D86;\n}\n\n/* Iframe video setting */\niframe{\n    width: 90vw;\n    height:50vw;\n    max-width: 600px;\n    max-height: 400px;\n}\n\n\n/* header */\n\nnav p {\n    color:#f4f3f1;\n    margin:auto;\n    font-size:4vw;\n}\nnav h1 {\n    color:#C0BBFF;\n    margin:auto;\n    font-size: 5vw;\n}\n#nav-options {\n    background-color: #f4f3f1;\n    text-align: center;\n}\n#nav-options ul{\n    font-size: 4vw;\n    list-style: none; \n    border-bottom:1px solid #000;\n}\n#nav-options ul li {\n    margin: 3vw auto;\n}\n\n#nav-options ul li a {\n    color:#433D86;\n    font-weight:800;\n}\n#nav-options ul li a:hover{\n    color:#376b3d;\n    \n}\n#nav-options .here{\n    color:#376b3d;\n    cursor:default;\n}\n#title{\n    padding:3vw;\n    background-color:#000;\n}\n\n/* images: */\nimg{\n    cursor:pointer; \n    width: 80vw;\n    max-width: 500px;\n}\n\nfigure {\n    margin:0;\n    text-align: center;\n}\n.image-circle{\n    border-radius: 50%;\n}\nfigcaption{\n    font-size:2vw;\n    line-height: 15px;\n}\n.imageSelected{\n    border-radius: 0%;\n    width:90vw;\n    border:5px solid #433D86;\n}\n\n/* position tools */\n.centered{\n    text-align: center;\n}\n\n/* Adult Content*/\n.hidden{\n    display:none;\n}\n\n/* footer */\nfooter{\n    font-size: 12px;\n    background-color:#000;\n    color:#C0BBFF;\n    padding:6vw 6vw 10vw 6vw;\n    margin-top: 50px;\n}\nfooter ul {\n    list-style: none;\n}\nfooter ul li a{\n    color: #C0BBFF;\n}\n#goUp{\n    position:relative;\n    float:right;\n    bottom: 50px;\n    right:20px;\n    font-size: 10vw;\n}\np{\n    text-align: justify;\n}\n\n/* form */\nlabel input{\n    margin:10px;\n}\n\n/* index */\n\n#index img{\n    width:40vw;\n}\n\n#index .imageSelected{\n    border-radius: 0%;\n    width:90vw;\n    border:5px solid #433D86;\n}\n\n/* book list */\n#bookDisplay figure img{\n    width: 80vw;\n    height: 80vw;\n}"},"lineCount":null}},"error":null,"hash":"762322605e428ca84837efc481ee3266","cacheData":{"env":{}}}